{
  "name": "evermind-domain",
  "version": "0.1.0",
  "description": "Domain code for evermind",
  "main": "build/use-cases/index.js",
  "type": "module",
  "scripts": {
    "tests": "uvu -r tsm tests",
    "compile": "tsc -p tsconfig.build.json",
    "tsc:watch": "tsc --noEmit --watch",
    "create-index": "ls src/use-cases/ | grep -v index[.]ts | sed s/[.]ts// | awk -v q=\"'\" '{print \"export {\" $0 \"} from \" q \"./\" $0 \".js\" q \";\"}' | sed '1s;^;// File created automatically. Do not modify manually\\n;' > ./src/use-cases/index.ts",
    "remove-index": "rm ./src/use-cases/index.ts",
    "build": "npm run create-index && npm run compile; npm run remove-index"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ferllop/evermind-domain.git"
  },
  "keywords": [
    "evermaind",
    "domain"
  ],
  "author": "Ferran Llop Alonso",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/ferllop/evermind-domain/issues"
  },
  "homepage": "https://github.com/ferllop/evermind-domain#readme",
  "devDependencies": {
    "@tsconfig/node16": "1.0.2",
    "@types/node": "16.4.10",
    "@types/pg": "8.6.4",
    "@types/pg-format": "1.0.2",
    "tsm": "2.2.1",
    "typescript": "4.5.5",
    "uvu": "0.5.3"
  },
  "dependencies": {
    "in-memory-database": "github:ferllop/in-memory-typescript-database#semver:1.x.x",
    "node-postgres": "0.6.2",
    "pg": "8.7.1",
    "pg-format": "1.0.4",
    "preconditions": "github:ferllop/preconditions#semver:2.0.1"
  }
}
